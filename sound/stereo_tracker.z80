	srl h
	rr l
	srl h
	rr l
	srl h
	rr l
	srl h
	rr l
	srl h
	rr l
	ret;109/349
	
	
fast_divmod_12: ;divide a by 12
	;result in b
	;modulus in a
	ld l,a
	ld e,a
	ld d,0
	ld h,d ;init ;; 19
	
	add hl,hl
	add hl,hl
	add hl,de 
	add hl,hl
	add hl,hl
	add hl,de ;times 21 ;; 66

	ld l,h
	ld e,h
	ld h,d ;rs 8 ;; 12
	ld b,l ;save divisor ;; 4
	
	add hl,hl
	add hl,de
	add hl,hl
	add hl,hl ;times 12 ;; 44

	sub h
	cp 12
	ret c ;  ;;22/16
	inc b
	sub 12
	ret ;; 21 ;total: 167/182
	
