dw 0,0x43ce,0x47d6,0x4c1c,0x50a3,0x556e,0x5a82,0x5fe4,0x6598,0x6ba2,0x7209,0x78d1,0x8000
note_pitch:
	ld b,a
	and $0f ;semitone
	add a,a
	add a,pitch_table & 0xff
	ld l,a
	ld h,pitch_table >> 8
	ld a,(hl)
	inc l
	ld h,(hl)
	ld l,a ;;55
	; got pitch, now rightshift the right amount
	ld a,b
	rra;4 bytes per
	rra
	and $3c
	ld ($+3+1),a  
	jr $+2
	srl h
	rr l
	srl h
	rr l
	srl h
	rr l
	srl h
	rr l
	srl h
	rr l
	srl h
	rr l
